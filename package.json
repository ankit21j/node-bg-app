{
  "name": "myservice",
  "version": "1.1.0",
  "description": "create and manage LAPP users",
  "main": "./dist/server.js",
  "scripts": {
    "clean": "rimraf dist/",
    "ts-node": "ts-node",
    "tslint": "tslint --project ./",
    "tsc": "tsc --project ./tsconfig.json",
    "prettier-all": "prettier --write 'src/**/*.+(ts|tsx|js|css|json)'",
    "precompile": "npm run prettier-all && npm run tslint && npm run clean",
    "compile": "npm run tsc",
    "pretest": "npm run compile",
    "build": "npm run test",
    "dev": "ts-node ./src/server.ts",
    "postbuild": "rimraf  dist/**/spec dist/**/*.spec.js",
    "debug": "cross-var node --inspect-brk=0.0.0.0:${DEBUG_PORT} --require source-map-support/register ./dist/server.js",
    "start": "node ./dist/server.js",
    "profile": "node --prof ./dist/server.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false
  },
  "author": "shasak",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/chai": "4.0.10",
    "@types/express": "4.11.1",
    "@types/istanbul": "0.4.29",
    "@types/jsonwebtoken": "7.2.6",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "3.1.4",
    "@types/node": "8.5.5",
    "@types/ramda": "^0.25.36",
    "@types/winston": "2.3.7",
    "@types/xml2js": "^0.4.2",
    "chai": "4.1.2",
    "istanbul": "0.4.5",
    "mocha": "^5.2.0",
    "nyc": "11.4.1",
    "prettier": "1.9.2",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.6",
    "ts-node": "4.1.0",
    "tslint": "^5.11.0",
    "tslint-clean-code": "^0.2.7",
    "typescript": "3.0.1",
    "yarn": "1.3.2",
    "cross-var": "1.1.0"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "^1.7.3",
    "conset": "2.0.3",
    "dotenv": "4.0.0",
    "express": "4.16.3",
    "hashids": "^1.1.4",
    "helmet": "3.9.0",
    "http-errors": "1.6.3",
    "moment": "2.20.1",
    "mongodb": "3.1.6",
    "mongoose": "^5.3.13",
    "ramda": "0.25.0",
    "winston": "2.4.0"
  }
}
